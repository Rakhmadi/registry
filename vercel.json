{
  "redirects": [
    {
      "source": "/",
      "destination": "https://github.com/denoland-id/registry"
    }
  ],
  "rewrites": [
    {
      "source": "/schema.json",
      "destination": "https://raw.githubusercontent.com/denoland-id/registry/schema.json"
    },
    {
      "source": "/:registry",
      "destination": "/api/registry"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
